function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(220);
  
  for (var i = 20; i <= 1200; i = i + 40) {
  // var startX = 20;
  // var startY = i;
  // var endX = 380;
  // var endY = i;
  noFill();
	strokeWeight(10)
  var radius = i
  ellipse(200, 0, radius);
	}
  
  
}
